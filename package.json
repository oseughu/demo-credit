{
  "name": "demo-credit",
  "version": "1.0.0",
  "description": " a demo credit app for lendsqr assessment",
  "main": "build/app.js",
  "type": "module",
  "imports": {
    "#src/*": "./src/*.js",
    "#controllers/*": "./src/controllers/*.js",
    "#database/*": "./src/database/*.js",
    "#services/*": "./src/services/*.js",
    "#middleware/*": "./src/middleware/*.js",
    "#utils/*": "./src/utils/*.js",
    "#routes": "./src/routes/index.js",
    "#routes/*": "./src/routes/*.js"
  },
  "scripts": {
    "start": "node src/app",
    "dev": "nodemon src/app",
    "migrate": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --knexfile src/database/knexfile.js",
    "test": "mocha --timeout 100000 ./src/tests/**/*.test.js",
    "knex": "./node_modules/.bin/knex --knexfile src/database/knexfile.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.7",
    "connect-session-knex": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.3.0",
    "mocha": "^10.2.0",
    "mysql2": "^2.3.3",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
